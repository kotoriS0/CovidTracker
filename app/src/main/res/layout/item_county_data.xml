<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_countyItem"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView_countyItem_county"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/textView_countyItem_weeklyCasesPer100k"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="15dp"
        tools:text="County" />

    <TextView
        android:id="@+id/textView_countyItem_lastUpdatedDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/textView_countyItem_county"
        app:layout_constraintTop_toBottomOf="@+id/textView_countyItem_county"
        tools:text="01/02/01" />

    <TextView
        android:id="@+id/textView_countyItem_weeklyCasesPer100k"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="15dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView_countyItem_county"
        tools:text="50.5" />
</androidx.constraintlayout.widget.ConstraintLayout>